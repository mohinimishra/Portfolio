<div class="container">
    <div class="nav-scroller py-1 mb-2">
        <!-- <nav class="nav d-flex justify-content-between">
            <% categories.forEach((ele)=>{ %>
            <a class="p-2 text-muted" href="/blogDetails"><%= ele %></a>
            <% })%>
        </nav> -->
    </div>

    <div class="jumbotron p-3 p-md-5 mb-4 text-white rounded bg-dark">
        <div class="col-md-6 px-0">
            <h1 class="display-4 font-italic"><%= featuredBlog.name %></h1>
            <p class="lead my-3"><%= featuredBlog.description %></p>
            <p class="lead mb-0"> <a href="" class=" text-white font-weight-bold">Continue reading...</a></p>
        </div>
    </div>

    <div class="row mb-2">
        <% blogData.forEach((ele) =>{ %>
        <div class="col-md-12">
            <div class="card flex-md-row mb-4 box-shadow h-md-250">
                <div class="card-body d-flex flex-column align-items-start">

                    <strong class="d-inline-block mb-2 text-primary"><%= ele.tag.name %></strong>
                    <h3 class="mb-0">
                        <a class="text-dark" href="/blogDetails"><%= ele.name %></a>
                    </h3>

                    <div class="mb-1 text-muted">Nov 12</div>
                    <p class="card-text mb-auto"><%= ele.description%></p>
                    <a href="/blogDetails/<%=ele.alias%>">Continue reading</a>
                </div>
                <img src="/image/images.jpeg" alt="" srcset="">
            </div>
        </div>
        <% })%>

    </div>

</div>


