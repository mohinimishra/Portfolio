<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3">
    <h1 class="h2">Projects</h1>
    <div class="btn-toolbar mb-2 mb-md-0">
        <div class="btn-group mr-2">
            <a href="/admin/addProject"><button class="btn btn-sm btn-outline-secondary">ADD +</button></a>
        </div>
    </div>
</div>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>No.</th>
                <th>Image</th>
                <th>Project</th>
                <th>Github Link</th>
                <th>Header</th>
            </tr>
        </thead>
        <tbody>
            <% project.forEach((ele, index)=>{ %>
            <tr>
                <td><%= index +1 %></td>
                <td><img src="/image/images.jpeg" width="80px" height="80px"></td>
                <td><a href="project/<%= ele.alias %>"><%= ele.name %></a></td>
                <td><a href="<%= ele.githubUrl %>">Github</a></td>
                <td><a href="#">Header</a></td>


            </tr>
            <% }) %>

        </tbody>
    </table>
</div>