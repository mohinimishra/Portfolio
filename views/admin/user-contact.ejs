<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>No.</th>
                <th>Name</th>
                <th>Email Id</th>
                <th>Mobile</th>
                <th>Description</th>

            </tr>
        </thead>
        <tbody>
            <% data.forEach((ele, index)=>{ %>
            <tr>
                <td><%= index +1 %></td>
                <td><%= ele.name %></td>
                <td><%= ele.email %></td>
                <td><%= ele.mobile %></td>
                <td><%= ele.description %></td>

            </tr>
            <% }) %>

        </tbody>
    </table>
</div>