<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3">
    <h1 class="h2">User's Information</h1>
    <div class="btn-toolbar mb-2 mb-md-0">
        <div class="btn-group mr-2">
            <a href="/admin/"><button class="btn btn-sm btn-outline-secondary">ADD+</button></a>
        </div>
    </div>
</div>

<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>No.</th>
                <th>Name</th>
                <th>Email Id</th>
                <th>Mobile</th>
                <th>Description</th>

            </tr>
        </thead>
        <tbody>
            <% data.forEach((ele, index)=>{ %>
            <tr>
                <td><%= index +1 %></td>
                <td><%= ele.name %></td>
                <td><%= ele.email %></td>
                <td><%= ele.mobile %></td>
                <td><%= ele.description %></td>

            </tr>
            <% }) %>

        </tbody>
    </table>
</div>